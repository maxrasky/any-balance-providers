<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id version="2">ab-wallet-mobilnik</id>
	<api flags="no_browser"/>
	<name>Wallet Mobilnik Balance</name>
	<description html="true"><![CDATA[
	<p>Предоставляет ФИО, номер счета и баланс пользователя mobilnik.kg со страницы https://wallet.mobilnik.kg/wallet/</p>
	]]></description>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>library.js</js>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="balance" name="Баланс" suffix=" KGS"/>
		<counter id="account_num" name="Лицевой счет" type="text"/>
		<counter id="username" name="ФИО" type="text"/>
	</counters>
	<keywords>мобильник, мобильный кошелек, wallet</keywords>
	<type>money, wallet</type>
	<country>kg</country>
</provider>